{
  "trailingComma": "all",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "printWidth": 120,
  "bracketSpacing": false,
  "endOfLine": "crlf",
  "importOrder": [
    "next/(.*)$",
    "^react(-dom(.)*)?$",
    "<THIRD_PARTY_MODULES>",
    "^@app/(.*)$",
    "^@components/(.*)$",
    "^@public/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
